package exercise;

public class ExerciseLoop1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
			for (int number =2; number <20; number++)	
			{
				number = number *2;
				
				if (number <15)
					System.out.println("Small number =" + number);
				else
					System.out.println("Big number =" + number);
				
			}


	}
