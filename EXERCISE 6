package exercise;

public class ExerciseLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i=1;
		
		 do
		 {	
			   if ((i % 2)== 0)
				System.out.println("Baki 0: " + i);
			   else
				   System.out.println("Baki selain 0: " + i);
			   i++;
		    } 
		 while(i<5);


	}

}
